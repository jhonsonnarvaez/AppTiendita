<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppTiendita.Principal"
             xmlns:pages="clr-namespace:AppTiendita"
            BackgroundColor="#cde2f2"
             NavigationPage.HasNavigationBar="False"
            >
    <FlyoutPage.Flyout>
        <pages:PrincipalFlyout x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:PrincipalDetail />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>